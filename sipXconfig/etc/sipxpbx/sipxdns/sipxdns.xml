<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="named-config">
    <group name="forwarders">
      <setting name="forwarder_0">
        <type><ipaddr/></type>
      </setting>
      <setting name="forwarder_1">
        <type><ipaddr/></type>
      </setting>
      <setting name="forwarder_2">
        <type><ipaddr/></type>
      </setting>
      <setting name="forwarder_3">
        <type><ipaddr/></type>
      </setting>
      <setting name="forwarder_4">
        <type><ipaddr/></type>
      </setting>
    </group>
  </group>
  <group name="acl">
    <setting name="ips">
      <type>
        <string multiLine="yes" maxLen="1000"/>
      </type>
    </setting>
  </group>
  <group name="sys" advanced="yes">       
    <setting name="unmanaged">
      <type>
        <boolean/>
      </type>
    </setting>
    <group name="unmanaged_servers">
      <setting name="unmanaged_0">
        <type>
          <hostname />
        </type>
      </setting>
      <setting name="unmanaged_1">
        <type>
          <hostname />
        </type>
      </setting>
      <setting name="unmanaged_2">
        <type>
          <hostname />
        </type>
      </setting>
      <setting name="unmanaged_3">
        <type>
          <hostname />
        </type>
      </setting>
     </group>
  </group>
</model>
