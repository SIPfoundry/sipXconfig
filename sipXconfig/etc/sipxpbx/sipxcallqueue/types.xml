<type id="type_strategy">
    <enum>
	<option><value>ring-all</value></option>
	<option><value>longest-idle-agent</value></option>
	<option><value>round-robin</value></option>
	<option><value>top-down</value></option>
	<option><value>agent-with-least-talk-time</value></option>
	<option><value>agent-with-fewest-calls</value></option>
	<option><value>sequetially-by-agent-order</value></option>
	<option><value>random</value></option>
    </enum>
</type>
<type id="type_upload-music">
	<file
	    variable="yes"
	    directory="/var/sipxdata/sipxcallqueue/music"
	/>
</type>
<type id="type_queue-type">
    <enum>
	<option><value>callback</value></option>
	<option><value>uuid-standby</value></option>
    </enum>
</type>
<type id="type_agent-status">
    <enum>
	<option><value>Logged Out</value></option>
	<option><value>Available</value></option>
	<option><value>Available(On Demand)</value></option>
	<option><value>On Break</value></option>
    </enum>
</type>
