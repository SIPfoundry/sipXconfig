<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd" [
    <!ENTITY types SYSTEM "types.xml">
    ]>
<model>
    &types;
    <group name="call-queue-agent">
	<setting name="max-no-answer"><type><integer/></type><value>0</value></setting>
	<setting name="wrap-up-time"><type><integer/></type><value>0</value></setting>
	<setting name="reject-delay-time"><type><integer/></type><value>0</value></setting>
	<setting name="busy-delay-time"><type><integer/></type><value>0</value></setting>
	<setting name="no-answer-delay-time"><type><integer/></type><value>0</value></setting>
    </group>
    <group name="call-queue-agent-code">
	<setting name="agent-login"><type><string/></type><value>*86</value></setting>
	<setting name="agent-logout"><type><string/></type><value>*88</value></setting>
	<setting name="agent-on-break"><type><string/></type><value>*84</value></setting>
    </group>
</model>
