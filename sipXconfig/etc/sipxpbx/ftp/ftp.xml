<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="true-false">
    <boolean>
      <true><value>YES</value></true>
      <false><value>NO</value></false>
    </boolean>
  </type>
  <group name="vsftp-config">
    <setting name="pasv_address">
      <type><ipaddr/></type>
    </setting>
    <setting name="pasv_addr_resolve">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="pasv_min_port">
      <type><integer /></type>
      <value>50000</value>
    </setting>
    <setting name="pasv_max_port">
      <type><integer /></type>
      <value>50050</value>
    </setting>
    <setting name="xferlog_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="syslog_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="reverse_lookup_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="anonymous_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="anon_upload_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="anon_mkdir_write_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
    <setting name="anon_other_write_enable">
      <type refid="true-false"/>
      <value>NO</value>
    </setting>
 </group>
</model>
