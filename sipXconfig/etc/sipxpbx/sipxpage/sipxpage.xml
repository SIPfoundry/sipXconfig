<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <group name="page-dial">
    <setting name="prefix">
      <value>*77</value>
    </setting>    
  </group>
  <group name="page-config">
    <setting name="sip.tcpPort" hidden="yes">
      <value>5160</value>
    </setting>
    <setting name="sip.udpPort" hidden="yes">
      <value>5160</value>
    </setting>
    <setting name="sip.tlsPort" hidden="yes">
      <value>5161</value>
    </setting>
    <setting name="log.level" hidden="yes">
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="sip.trace" advanced="yes">
      <type>
        <enum>
          <option>
            <value>NONE</value>
          </option>
          <option>
            <value>TRACE</value>
          </option>
          <option>
            <value>DEBUG</value>
          </option>
        </enum>
      </type>
      <value>NONE</value>
    </setting>
  </group>
</model>