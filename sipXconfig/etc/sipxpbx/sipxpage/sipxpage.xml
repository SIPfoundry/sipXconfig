<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <type id="nonRootRequiredPort">
    <integer min="1025" max="65535"/>
  </type>
  <group name="page-config">
    <setting name="prefix">
      <value>*77</value>
    </setting>
    <setting name="sip.trace">
      <type>
        <enum>
          <option>
            <value>NONE</value>
          </option>
          <option>
            <value>TRACE</value>
          </option>
          <option>
            <value>DEBUG</value>
          </option>
        </enum>
      </type>
      <value>NONE</value>
    </setting>  
    <setting name="sip.tcpPort" advanced="yes">
      <type refid="nonRootRequiredPort"/>
      <value>5160</value>
    </setting>
    <setting name="sip.udpPort" advanced="yes">
      <type refid="nonRootRequiredPort"/>
      <value>5160</value>
    </setting>
    <setting name="sip.tlsPort" advanced="yes">
      <type refid="nonRootRequiredPort"/>
      <value>5161</value>
    </setting>
    <setting name="rtp.port" advanced="yes">
      <type refid="nonRootRequiredPort"/>
      <value>8500</value>
    </setting>
    <setting name="log.level" advanced="yes">
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
  </group>
</model>