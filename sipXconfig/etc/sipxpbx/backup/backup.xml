<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="ftp">  
    <setting name="url">
      <type><string required="yes"/></type>
    </setting>
    <setting name="user">
      <type><string required="yes"/></type>
    </setting>
    <setting name="password" >
      <type><password/></type>
    </setting>
  </group>
</model>
