<% 
  if LOGIN_FAILED[:groupName] != 'disabled'
%>
type=SingleWithThreshold
ptype=RegExp
pattern=Authentication event AuthenticationFailureBadCredentialsEvent: (\S+)
desc=Too many failed login attempts for user $1
action=shellcmd @SIPX_BINDIR@/sipxtrap sipxecsalarm '%s'
window=60
thresh=<%= LOGIN_FAILED[:minThreshold] %>
<% 
  end 
%>
