<% 
  if LOGIN_FAILED[:groupName] != 'disabled'
%>
type=SingleWithThreshold
ptype=RegExp
pattern=Authentication event AuthenticationFailureBadCredentialsEvent: (\S+); .* RemoteIpAddress: (\S+);
desc=<%= LOGIN_FAILED[:minThreshold] %> failed login attempts for user $1 within a 60 second interval.
action=shellcmd @SIPX_BINDIR@/sipxtrap LOGIN_FAILED '%s'
window=60
thresh=<%= LOGIN_FAILED[:minThreshold] %>
<% 
  end 
%>
<% 
  if PHONE_ADDED[:groupName] != 'disabled'
%>
type=Single
ptype=RegExp
pattern=ALARM_PHONE_ADDED Phone with serial (.*) was added to the system.
desc=Phone with serial $1 added. 
action=shellcmd @SIPX_BINDIR@/sipxtrap ALARM_PHONE_ADDED '%s'
<% 
  end 
%>
<% 
  if PHONE_CHANGED[:groupName] != 'disabled'
%>
type=Single
ptype=RegExp
pattern=ALARM_PHONE_CHANGED Phone with id: (.*), serial: (.*) was changed.
desc=Phone with id $1 and serial $2 changed. 
action=shellcmd @SIPX_BINDIR@/sipxtrap ALARM_PHONE_CHANGED '%s'
<% 
  end 
%>
<% 
  if PHONE_DELETED[:groupName] != 'disabled'
%>
type=Single
ptype=RegExp
pattern=ALARM_PHONE_DELETED Phone with id: (.*), serial: (.*) was deleted.
desc=Phone with id $1 and serial $2 deleted. 
action=shellcmd @SIPX_BINDIR@/sipxtrap ALARM_PHONE_DELETED '%s'
<% 
  end 
%>
<% 
  if BACKUP_FAILED[:groupName] != 'disabled'
%>
type=Single
ptype=RegExp
pattern=Backup FAILED for configuration: (.*)
desc=Backup with configuration $1 failed
action=shellcmd @SIPX_BINDIR@/sipxtrap BACKUP_FAILED '%s'
<% 
  end 
%>
<% 
  if LDAP_IMPORT_FAILED[:groupName] != 'disabled'
%>
type=Single
ptype=RegExp
pattern=LDAP users imported: (.*) rejected: (.*)
desc=LDAP import could not import all users. SUCCESS: $1 FAILURE: $2
action=shellcmd @SIPX_BINDIR@/sipxtrap LDAP_IMPORT_FAILED '%s'
<% 
  end 
%>
