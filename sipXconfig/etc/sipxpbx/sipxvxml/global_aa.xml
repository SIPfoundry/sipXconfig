<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="liveAttendant">
    <setting name="did">
      <value />
    </setting>
    <setting name="enablePrefix">
      <value>*67</value>
    </setting>
    <setting name="disablePrefix">
      <value>*68</value>
    </setting>
    <setting name="expireTime">
      <type>
        <integer />
      </type>
      <value />
    </setting>
  </group>
  <group name="dtmf">
    <setting name="maxDigits">
      <type>
        <integer min="1" required="yes" />
      </type>
      <value>20</value>
    </setting>
    <setting name="firstDigitTimeout">
      <type>
        <integer min="1" required="yes" />
      </type>
      <value>10</value>
    </setting>
    <setting name="interDigitTimeout">
      <type>
        <integer min="1" required="yes" />
      </type>
      <value>1</value>
    </setting>
    <setting name="extraDigitTimeout">
      <type>
        <integer min="1" required="yes" />
      </type>
      <value>1</value>
    </setting>
  </group>
</model>
