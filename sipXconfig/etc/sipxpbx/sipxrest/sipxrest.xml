<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <group name="rest-config">
    <setting name="sipPort" advanced="yes">
      <type>
        <integer />
      </type>
      <value>6050</value>
    </setting>
    <setting name="httpsPort">
      <type>
        <integer />
      </type>
      <value>6666</value>
    </setting>
    <setting name="extHttpPort" advanced="yes">
      <type>
        <integer />
      </type>
      <value>6667</value>
    </setting>
    <setting name="SIPX_REST_LOG_LEVEL" hidden="yes">
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
  </group>
</model>
