<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
     <enum>
      <option><value>3</value></option>
      <option><value>2</value></option>
      <option><value>1</value></option>
      <option><value>0</value></option>
      <option><value>-1</value></option>
      <option><value>-2</value></option>
      <option><value>-3</value></option>
      <option><value>-4</value></option>
      <option><value>-1</value></option>
    </enum>
  </type>
  <type id="pluginLogLevel">
     <enum>
      <option><value>0</value></option>
      <option><value>1</value></option>
      <option><value>2</value></option>
      <option><value>3</value></option>
      <option><value>4</value></option>
      <option><value>5</value></option>
      <option><value>6</value></option>
      <option><value>7</value></option>
      <option><value>8</value></option>
    </enum>
  </type>
  <type id="true-false">
	<boolean>
		<true>
			<value>true</value>
		</true>
		<false>
			<value>false</value>
		</false>
	</boolean>
  </type>     
  <group name="kamailio-configuration">
    <setting name="LOG_LEVEL">
      <type refid="loglevel" />
      <value>1</value>
    </setting>
    <setting name="TABLE_DIALOG_VERSION" hidden="yes">
      <type>
        <integer />
      </type>
      <value>7</value>
    </setting>
    <setting name="TABLE_DIALOG_VARS_VERSION" hidden="yes">
      <type>
        <integer />
      </type>
      <value>1</value>
    </setting>
	<setting name="DEFAULT_SHARED_MEMORY">
      <type>
        <integer />
      </type>
      <value>256</value>
    </setting>
	<setting name="DEFAULT_PRIVATE_MEMORY">
      <type>
        <integer />
      </type>
      <value>64</value>
    </setting>        
    <setting name="DEFAULT_CHILDREN" advanced="yes">
      <type>
        <integer />
      </type>
      <value>16</value>
    </setting>
    <setting name="TCP_WRITE_QUEUE_SIZE" advanced="yes">
      <type>
        <integer />
      </type>
      <value>65536</value>
    </setting>
    <setting name="TCP_READ_BUFFER_SIZE" advanced="yes">
      <type>
        <integer />
      </type>
      <value>65536</value>
    </setting>
	<setting name="TCP_CONNECTION_LIFETIME" hidden="yes">
      <type>
        <integer />
      </type>
      <value>3605</value>
    </setting>
    <setting name="DEFAULT_DUMP_CORE" >
      <type refid="true-false" />
      <value>false</value>
    </setting>                        
  </group>
  <group name="kamailio-proxy-configuration">
    <setting name="TCP_PORT" hidden="yes">
      <type>
        <integer />
      </type>
      <value>5060</value>
    </setting>
    <setting name="UDP_PORT" hidden="yes">
      <type>
        <integer />
      </type>
      <value>5060</value>
    </setting>
    <setting name="TLS_PORT" hidden="yes">
      <type>
        <integer />
      </type>
      <value>5061</value>
    </setting>
  </group>
  <group name="kamailio-presence-configuration">
    <setting name="TCP_PORT" hidden="yes">
      <type>
        <integer />
      </type>
      <value>5065</value>
    </setting>
    <setting name="UDP_PORT" hidden="yes">
      <type>
        <integer />
      </type>
      <value>5065</value>
    </setting>
    <setting name="TLS_PORT" hidden="yes">
      <type>
        <integer />
      </type>
      <value>5066</value>
    </setting>
    <setting name="ENABLE_BLF_SIPX_PLUGIN">
      <type refid="true-false" />
      <value>true</value>
    </setting>
    <setting name="BLF_SIPX_PLUGIN_LOG_SETTING">
      <type refid="pluginLogLevel" />
      <value>6</value>
    </setting>
    <setting name="ENABLE_BLA_MESSAGE_QUEUE" hidden="yes">
      <type refid="true-false" />
      <value>true</value>
    </setting>
    <setting name="ENABLE_POLL_BLA_USER_SETTING" hidden="yes">
      <type refid="true-false" />
      <value>false</value>
    </setting>
    <setting name="BLA_USER_POLL_INTERVAL_SETTING" hidden="yes">
      <type>
        <integer />
      </type>
      <value>60</value>
    </setting>
  </group>
</model>
