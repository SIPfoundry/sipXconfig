[Unit]
Description=kamailio-proxy
Documentation=http://sipfoundry.org
After=network.target syslog.target

[Service]
# see man systemd.service
Type=oneshot
ExecStart=@SIPX_SERVICEDIR@/kamailio-proxy start
RemainAfterExit=true
ExecStop=@SIPX_SERVICEDIR@/kamailio-proxy stop
StandardOutput=journal

[Install]
WantedBy=multi-user.target