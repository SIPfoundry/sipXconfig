  <!--  CALL PARK SETTINGS  -->
  <group name="callPark">
    <setting name="callParkMethod">
      <type>
        <enum>
          <option>
            <value>none</value>
          </option>
          <option>
            <value>prefix</value>
          </option>
          <option>
            <value>incall DTMF</value>
          </option>
        </enum>
      </type>
      <value>none</value>
    </setting>
    <setting name="callParkExtension">
      <type>
        <string maxLen="22" />
      </type>
      <value />
    </setting>
    <setting name="callRtrvPrefix">
      <type>
        <string maxLen="22" />
      </type>
      <value />
    </setting>
  </group>
  <!--  CALL PICKUP SETTINGS  -->
  <group name="callPickup">
    <setting name="serverAssistCallPickup">
      <type refid="on-off" />
      <value>TRUE</value>
    </setting>
    <setting name="callPickupPrefix">
      <type>
        <string />
      </type>
      <value />
