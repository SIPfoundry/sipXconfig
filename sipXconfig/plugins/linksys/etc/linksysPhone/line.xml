<?xml version="1.0"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="Ext">
    <label>Extensions</label>
    <setting name="Line_Enable">
      <label>Line Enable</label>
      <profileName>General</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="NAT_Mapping_Enable">
      <label>NAT Mapping Enable</label>
      <profileName>NAT_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="NAT_Keep_Alive_Enable">
      <label>NAT Keep Alive Enable</label>
      <profileName>NAT_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="User_ID">
      <label>User ID</label>
      <profileName>Subscriber_Information</profileName>
      <value />
    </setting>
    <setting name="Display_Name" advanced="yes">
      <label>Display Name</label>
      <profileName>Subscriber_Information</profileName>
      <value />
    </setting>
    <setting name="Password" advanced="yes">
      <label>Password</label>
      <profileName>Subscriber_Information</profileName>
      <type>
        <string password="yes" />
      </type>
      <value />
    </setting>
    <setting name="Proxy">
      <label>Proxy</label>
      <profileName>Proxy_and_Registration</profileName>
      <value />
    </setting>
    <setting name="Register">
      <label>Register</label>
      <profileName>Proxy_and_Registration</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Make_Call_Without_Reg">
      <label>Make Call Without Reg</label>
      <profileName>Proxy_and_Registration</profileName>
      <type>
        <enum>
          <option>
            <value>no</value>
          </option>
          <option>
            <value>yes</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Register_Expires">
      <label>Register Expires</label>
      <profileName>Proxy_and_Registration</profileName>
      <type>
        <integer />
      </type>
      <value>3600</value>
    </setting>
    <setting name="SIP_Port">
      <label>SIP Port</label>
      <profileName>SIP_Settings</profileName>
      <type>
        <integer />
      </type>
      <value>5060</value>
    </setting>
    <setting name="Message_Waiting">
      <label>Message Waiting</label>
      <profileName>Call_Feature_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Default_Ring">
      <label>Default Ring</label>
      <profileName>Call_Feature_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>No Ring</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Mailbox_ID">
      <label>Mailbox ID</label>
      <profileName>Call_Feature_Settings</profileName>
      <value />
    </setting>
  </group>
</model>
