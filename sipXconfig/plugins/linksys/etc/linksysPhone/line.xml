<?xml version="1.0"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="Extension_1">
    <setting name="Line_Enable_1_">
      <label>Line Enable</label>
      <profileName>Ext_1/General</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="NAT_Mapping_Enable_1_">
      <label>NAT Mapping Enable</label>
      <profileName>Ext_1/NAT_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="NAT_Keep_Alive_Enable_1_">
      <label>NAT Keep Alive Enable</label>
      <profileName>Ext_1/NAT_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="User_ID_1_">
      <label>User ID</label>
      <profileName>Ext_1/Subscriber_Information</profileName>
      <value />
    </setting>
    <setting name="Display_Name_1_" advanced="yes">
      <label>Display Name</label>
      <profileName>Ext_1/Subscriber_Information</profileName>
      <value />
    </setting>
    <setting name="Password_1_" advanced="yes">
      <label>Password</label>
      <profileName>Ext_1/Subscriber_Information</profileName>
      <type>
        <string password="yes" />
      </type>
      <value />
    </setting>
    <setting name="Proxy_1_">
      <label>Proxy</label>
      <profileName>Ext_1/Proxy_and_Registration</profileName>
      <value />
    </setting>
    <setting name="Register_1_">
      <label>Register</label>
      <profileName>Ext_1/Proxy_and_Registration</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Make_Call_Without_Reg_1_">
      <label>Make Call Without Reg</label>
      <profileName>Ext_1/Proxy_and_Registration</profileName>
      <type>
        <enum>
          <option>
            <value>no</value>
          </option>
          <option>
            <value>yes</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Register_Expires_1_">
      <label>Register Expires</label>
      <profileName>Ext_1/Proxy_and_Registration</profileName>
      <type>
        <integer />
      </type>
      <value>3600</value>
    </setting>
    <setting name="SIP_Port_1_">
      <label>SIP Port</label>
      <profileName>Ext_1/SIP_Settings</profileName>
      <type>
        <integer />
      </type>
      <value>5060</value>
    </setting>
    <setting name="Message_Waiting_1_">
      <label>Message Waiting</label>
      <profileName>Ext_1/Call_Feature_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Default_Ring_1_">
      <label>Default Ring</label>
      <profileName>Ext_1/Call_Feature_Settings</profileName>
      <type>
        <enum>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>No Ring</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value />
    </setting>
    <setting name="Mailbox_ID_1_">
      <label>Mailbox ID</label>
      <profileName>Ext_1/Call_Feature_Settings</profileName>
      <value />
    </setting>
  </group>
</model>
