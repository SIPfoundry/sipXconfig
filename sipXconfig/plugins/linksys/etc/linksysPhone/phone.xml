<?xml version="1.0"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="System">
    <setting name="Enable_Web_Server">
      <profileName>System/System_Configuration</profileName>
      <label>Enable Web Server</label>
      <type>
        <enum>
          <option>
            <value>yes</value>
          </option>
          <option>
            <value>no</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="Connection_Type">
      <profileName>System/Internet_Connection_Type_</profileName>
      <label>Connection Type</label>
      <type>
        <enum>
          <option>
            <value>DHCP</value>
          </option>
          <option>
            <value>Static IP</value>
          </option>
          <option>
            <value>PPPOE</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="Static_IP">
      <profileName>System/_Static_IP_Settings</profileName>
      <label>Static IP</label>
      <value />
