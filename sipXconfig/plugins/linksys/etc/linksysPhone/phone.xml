    <setting name="Speed_Dial_8">
      <profileName>User/Call_Forward</profileName>
      <label>Speed Dial 8</label>
      <value />
    </setting>
    <setting name="Speed_Dial_9">
      <profileName>User/Call_Forward</profileName>
      <label>Speed Dial 9</label>
      <value />
    </setting>
    <setting name="CW_Setting">
      <profileName>User/Supplementary_Services</profileName>
      <label>CW Setting</label>
      <type>
        <enum>
          <option>
            <value>yes</value>
