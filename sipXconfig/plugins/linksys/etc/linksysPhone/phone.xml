      <label>Gateway</label>
      <value />
    </setting>
    <setting name="HostName">
      <profileName>System/Optional_Network_Configuration</profileName>
      <label>HostName</label>
      <value />
    </setting>
    <setting name="Domain">
      <profileName>System/Optional_Network_Configuration</profileName>
      <label>Domain</label>
      <value />
    </setting>
    <setting name="Primary_DNS">
      <profileName>System/Optional_Network_Configuration</profileName>
      <label>Primary DNS</label>
      <value />
    </setting>
    <setting name="Secondary_DNS">
      <profileName>System/Optional_Network_Configuration</profileName>
      <label>Secondary DNS</label>
      <value />
    </setting>
    <setting name="DNS_Query_Mode">
      <profileName>System/System_Configuration</profileName>
      <label>DNS Query Mode</label>
      <type>
        <enum>
          <option>
            <value>Parallel</value>
          </option>
          <option>
            <value>Sequential</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="Syslog_Server">
      <profileName>System/Optional_Network_Configuration</profileName>
      <label>Syslog Server</label>
      <value />
    </setting>
    <setting name="Debug_Server">
      <profileName>System/Optional_Network_Configuration</profileName>
      <label>Debug Server</label>
      <value />
    </setting>
    <setting name="Debug_Level">
      <profileName>System/System_Configuration</profileName>
      <label>Debug Level</label>
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
        </enum>
      </type>
    </setting>
  </group>


  <group name="Regional">
    <setting name="Set_Local_Date_mm_dd_">
      <profileName>Regional/Miscellaneous</profileName>
      <label>Set Local Date (mm/dd)</label>
      <value />
    </setting>
    <setting name="Set_Local_Time_HH_mm_">
      <profileName>Regional/Miscellaneous</profileName>
      <label>Set Local Time (HH/mm)</label>
      <value />
    </setting>
    <setting name="Time_Zone">
      <profileName>Regional/Miscellaneous</profileName>
      <label>Time Zone</label>
      <value>GMT</value>
    </setting>
    <setting name="Time_Offset_HH_mm">
      <profileName>Regional/Miscellaneous</profileName>
      <label>Time Offset (HH/mm)</label>
      <value />
    </setting>
  </group>

  <group name="Phone">
    <setting name="Station_Name">
      <profileName>phone/General</profileName>
      <label>Station Name</label>
      <value />
    </setting>
    <setting name="Extension_1_">
      <profileName>phone/Line_Key_1</profileName>
      <label>Line Key 1 Extension</label>
      <value>1</value>
    </setting>
    <setting name="Short_Name_1_">
      <profileName>phone/Line_Key_1</profileName>
      <label>Line Key 1 Short Name</label>
      <value>$USER</value>
    </setting>
    <setting name="Extension_2_">
      <profileName>phone/Line_Key_2</profileName>
      <label>Line Key 2 Extension</label>
      <value>1</value>
    </setting>
    <setting name="Short_Name_2_">
      <profileName>phone/Line_Key_2</profileName>
      <label>Line Key 2 Short Name</label>
      <value>$USER</value>
    </setting>
    <setting name="Extension_3_">
      <profileName>phone/Line_Key_3</profileName>
      <label>Line Key 3 Extension</label>
      <value>1</value>
    </setting>
    <setting name="Short_Name_3_">
      <profileName>phone/Line_Key_3</profileName>
      <label>Line Key 1 Short Name</label>
      <value>$USER</value>
    </setting>
    <setting name="Extension_4_">
      <profileName>phone/Line_Key_4</profileName>
      <label>Line Key 4 Extension</label>
      <value>1</value>
    </setting>
    <setting name="Short_Name_4_">
      <profileName>phone/Line_Key_4</profileName>
      <label>Line Key 4 Short Name</label>
      <value>$USER</value>
    </setting>
    <setting name="SCA_Line_ID_Mapping">
      <profileName>phone/Miscellaneous_Line_Key_Setings</profileName>
      <label>SCA Line ID Mapping</label>
      <type>
        <enum>
          <option>
            <value>Vertical First</value>
          </option>
          <option>
            <value>Horizontal First</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="ACD_Ext">
      <profileName>phone/Supplementary_Services</profileName>
      <label>ACD Ext</label>
      <type>
        <enum>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>2</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="Voice_Mail_Number">
      <profileName>phone/General</profileName>
      <label>Voice Mail Number</label>
      <value />
    </setting>
    <setting name="Text_Logo">
      <profileName>phone/General</profileName>
      <label>Text Logo</label>
      <value />
    </setting>
  </group>


  <group name="User">
    <setting name="Cfwd_All_Dest">
      <profileName>User/Call_Forward</profileName>
