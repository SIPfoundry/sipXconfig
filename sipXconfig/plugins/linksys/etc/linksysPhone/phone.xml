        </enum>
      </type>
    </setting>
    <setting name="Block_CID_Setting">
      <profileName>User/Supplementary_Services</profileName>
      <label>Block CID Setting</label>
      <type>
        <enum>
          <option>
            <value>no</value>
          </option>
          <option>
            <value>yes</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="Block_ANC_Setting">
      <profileName>User/Supplementary_Services</profileName>
      <label>Block ANC Setting</label>
      <type>
        <enum>
          <option>
