<?xml version="1.0" encoding="UTF-8"?>
<resource-lists xmlns="urn:ietf:params:xml:ns:resource-lists" xmlns:cp="counterpath:properties">
<!--

   Contact Directory Configuration File

   Generated by sipXecs: http://www.sipfoundry.org/

-->
<list name="Contact List">
#foreach($entry in $cfg.Rows)
	<entry uri= "$entry.Uri">
	<cp:prop name="given_name" value="${entry.GivenName}" />
	<cp:prop name="surname" value="${entry.LastName}" />
	<cp:prop name="default_address" value="${entry.Uri}" />
	<cp:prop name="default_address_type" value="sip" />
	<cp:prop name="default_address_comm" value="phone" />
	<cp:prop name="default_address_moniker" value="${entry.Uri}" />
	<cp:prop name="sip_address" value="${entry.Uri}" />
	<cp:prop name="sip_address_comm" value="phone" />
	<cp:prop name="sip_address_moniker" value="${entry.Uri}" />
	<cp:prop name="business_number" value="${entry.BusinessNumber}" />
	<cp:prop name="xmpp_address" value="${entry.XmppAddress}" />
	<cp:prop name="home_number" value="${entry.HomeNumber}" />
	<cp:prop name="mobile_number" value="${entry.MobileNumber}" />
	<cp:prop name="fax_number" value="${entry.FaxNumber}" />
	<cp:prop name="postal_address" value="${entry.HomeAddress}" />
	<cp:prop name="home_postal_address" value="${entry.HomeAddress}"/>
	<cp:prop name="business_postal_address" value="${entry.OfficeAddress}" />
	<cp:prop name="company" value="${entry.Company}" />
	<cp:prop name="job_title" value="${entry.JobTitle}" />
	<cp:prop name="category" value="Work" />
	</entry>
#end
</list>
</resource-lists>
