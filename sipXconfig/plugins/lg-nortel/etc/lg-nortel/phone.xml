<?xml version="1.0"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="ena-dis">
    <boolean>
      <true>
        <value>enable</value>
      </true>
      <false>
        <value>disable</value>
      </false>
    </boolean>
  </type>
  <type id="on-off">
    <boolean>
      <true>
        <value>on</value>
      </true>
      <false>
        <value>off</value>
      </false>
    </boolean>
  </type>
  <type id="port-number">
    <integer required="yes" max="65534" />
  </type>
  <type id="day-of-week">
    <enum>
      <option>
        <label>Sunday</label>
        <value>1</value>
      </option>
      <option>
        <label>Monday</label>
        <value>2</value>
      </option>
      <option>
        <label>Tuesday</label>
        <value>3</value>
      </option>
      <option>
        <label>Wednesday</label>
        <value>4</value>
      </option>
      <option>
        <label>Thursday</label>
        <value>5</value>
      </option>
      <option>
        <label>Friday</label>
        <value>6</value>
      </option>
      <option>
        <label>Saturday</label>
        <value>7</value>
      </option>
    </enum>
  </type>
  <type id="month-of-year">
    <enum>
      <option>
        <label>January</label>
        <value>1</value>
      </option>
      <option>
        <label>February</label>
        <value>2</value>
      </option>
      <option>
        <label>March</label>
        <value>3</value>
      </option>
      <option>
        <label>April</label>
        <value>4</value>
      </option>
      <option>
        <label>May</label>
        <value>5</value>
      </option>
      <option>
        <label>June</label>
        <value>6</value>
      </option>
      <option>
        <label>July</label>
        <value>7</value>
      </option>
      <option>
        <label>August</label>
        <value>8</value>
      </option>
      <option>
        <label>September</label>
        <value>9</value>
      </option>
      <option>
        <label>October</label>
        <value>10</value>
      </option>
      <option>
        <label>November</label>
        <value>11</value>
      </option>
      <option>
        <label>December</label>
        <value>12</value>
      </option>
    </enum>
  </type>
  <group name='VOIP'>
    <setting name='outbound_proxy_server'></setting>
    <setting name='outbound_proxy_port' advanced="yes">
      <type refid="port-number" />
      <value>5060</value>
    </setting>
    <setting name='backup_proxy_server' advanced="yes"></setting>
    <setting name='backup_proxy_port' advanced="yes">
      <type refid="port-number" />
      <value>5060</value>
    </setting>
    <setting name='SIP_service_domain' advanced="yes"></setting>
    <setting name="moh_url" advanced="yes"></setting>
    <setting name='rtp_port' advanced="yes">
      <type refid="port-number" />
      <value>23000</value>
    </setting>
    <setting name='prack_mode' advanced="yes">
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='user_param' advanced="yes">
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='replaces_mode' advanced="yes">
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='local_udp_port' advanced="yes">
      <type refid="port-number" />
      <value>5060</value>
    </setting>
    <setting name='message_url' advanced="yes">
      <value></value>
    </setting>
    <setting name='preferred_codec'>
      <!--TODO: preferred codec list, g729/g723/pcmu/pcma (default: priority 1, g.729) -->
      <value>g729 g723 pcmu pcma</value>
    </setting>
    <setting name='timer_register_expires' advanced="yes">
      <type>
        <integer min="0" max="6400" />
      </type>
      <value>3600</value>
    </setting>
    <setting name='timer_session' advanced="yes">
      <type>
        <integer min="0" max="640" />
      </type>
      <value>0</value>
    </setting>
    <setting name='timer_t1'>
      <type>
        <integer />
      </type>
      <value>500</value>
    </setting>
    <setting name='timer_t2'>
      <type>
        <integer />
      </type>
      <value>4000</value>
    </setting>
    <setting name='stun_server'>
      <value></value>
    </setting>
    <setting name='max_line_num' hidden="yes">
      <type>
        <integer />
      </type>
    </setting>
  </group>
  <group name='DSP'>
    <setting name='ringer_off'>
      <type refid="on-off" />
      <value>on</value>
    </setting>
    <setting name='VAD'>
      <type refid="on-off" />
      <value>off</value>
    </setting>
    <setting name='dtmf_payload'>
      <type>
        <integer min="96" max="255" />
      </type>
      <value>101</value>
    </setting>
    <setting name='frame_duration'>
      <type>
        <integer min="10" max="100" />
      </type>
      <value>20</value>
    </setting>
    <setting name='headset_mode'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='dtmf'>
      <type>
        <enum>
          <option>
            <value>inband</value>
          </option>
          <option>
            <value>nego</value>
          </option>
          <option>
            <value>rfc2833</value>
          </option>
          <option>
            <value>info</value>
          </option>
        </enum>
      </type>
      <value>nego</value>
    </setting>
    <setting name='ringtype0'>
      <!--two value space separated array, 0~40000 Hz -->
      <value>425 440</value>
    </setting>
    <setting name='ringtype1'>
      <!--two value space separated array, 0~40000 Hz -->
      <value>530 555</value>
    </setting>
    <setting name='cadence0'>
      <!--6 value space separated array, 0~30000 msec -->
      <value>2000 4000 0 0 0 0</value>
    </setting>
    <setting name='cadence1'>
      <!--6 value space separated array, 0~30000 msec -->
      <value>800 400 800 4000 0 0</value>
    </setting>
    <setting name='cadence2'>
      <!--6 value space separated array, 0~30000 msec -->
      <value>400 200 400 200 800</value>
    </setting>
    <setting name='cadence3'>
      <!--6 value space separated array, 0~30000 msec -->
      <value>300 200 1000 200 300</value>
    </setting>
    <setting name='cadence4'>
      <!--6 value space separated array, 0~30000 msec -->
      <value>500 0 0 0 0 0</value>
    </setting>
    <setting name='jitterbuffer_min_delay'>
      <type>
        <integer min="0" max="150" />
      </type>
      <value>30</value>
    </setting>
    <setting name='jitterbuffer_nom_delay'>
      <type>
        <integer min="0" max="150" />
      </type>
      <value>60</value>
    </setting>
    <setting name='jitterbuffer_max_delay'>
      <type>
        <integer min="0" max="150" />
      </type>
      <value>120</value>
    </setting>
  </group>
  <group name='FWRD'>
    <setting name='condition'>
      <!---1=disable Fwd/0=none/1=no-answer/2=busy/3=no-answer & busy/4=un-conditional
      -->
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
          <option>
            <value>4</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>
    <setting name='forwarding_address'></setting>
    <setting name='noanswer_time'>
      <type>
        <integer min="0" max="30" />
      </type>
      <value>10</value>
    </setting>
  </group>
  <group name='DIAL'>
    <setting name='dial_mode'>
      <!--Dial Mode, number/ip/url (default: number)
      -->
      <type>
        <enum>
          <option>
            <value>number</value>
          </option>
          <option>
            <value>ip</value>
          </option>
          <option>
            <value>url</value>
          </option>
        </enum>
      </type>
      <value>number</value>
    </setting>
    <setting name='pause_timer'>
      <type>
        <integer min="1" max="30" />
      </type>
      <value>3</value>
    </setting>
    <setting name='inter_digit_timer'>
      <type>
        <integer min="1" max="30" />
      </type>
      <value>6</value>
    </setting>
  </group>
  <!-- 
    <group name='PROG' hidden="yes">
    Generated by speeddial functionality
    
    ;add prog func [num]     
    ;Entries for programmable buttons. Type the word “add” followed by space, the
    button number, space, function number and for speed dials the number to dial. For message function
    enter the url, ip address or telephone number for voice mail.
    
    prog = programmable button number, 6804N:3~4, 6812D:3~11, 6830D:3~24
    func = 0 for no function
    1 for Speed Dial
    2 for Phone Book
    3 for Call Log
    4 for Mute
    5 for Headset
    6 for Redial
    7 for Return
    8 for FWRD
    9 for CONF
    10 for TRNS
    11 for Hold
    12 for DND
    13 Ringer Off
    14 Message
    15 ACD Agent
    16 Intercom
    [num] =             ;Speed Dial phone number or Vmail contact, max.50 character string.
    
    </group>
  -->
  <group name='CALL'>
    <setting name='caller_id_blk'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='anonymous_call_blk'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='call_waiting'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='call_waiting_tone'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='call_hold_ringback'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='msg_waiting_tone'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='auto_answer'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='intercom_noti'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='acd_active'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='acd_primary_line' advanced="yes"></setting>
    <setting name='acd_unavail_completed' advanced="yes">
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='acd_unavail_missed' advanced="yes">
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='acd_unavail_time_completed'>
      <type>
        <integer min="0" max="999" />
      </type>
      <value>60</value>
    </setting>
    <setting name='call_direct'>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='call_direct_number'></setting>
    <setting name='call_direct_timer'>
      <type>
        <integer min="0" max="10" />
      </type>
      <value>5</value>
    </setting>
  </group>
  <group name='SNMP'>
    <setting name='snmp_function'>
      <type refid="on-off" />
      <value>off</value>
    </setting>
    <setting name='snmp_manager_address'>
      <value></value>
    </setting>
    <setting name='snmp_read_community'>
      <type>
        <string maxLen="31" password="yes" />
      </type>
    </setting>
    <setting name='snmp_write_community'>
      <type>
        <string maxLen="31" password="yes" />
      </type>
    </setting>
  </group>
  <group name='SYSTEM'>
    <setting name='lang'>
      <type>
        <enum>
          <option>
            <label>English</label>
            <value>0</value>
          </option>
          <option>
            <label>French</label>
            <value>1</value>
          </option>
          <option>
            <label>Korean</label>
            <value>2</value>
          </option>
          <option>
            <label>Danish</label>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>
    <setting name='lock_outgoing_call'>
      <type>
        <boolean>
          <true>
            <value>1</value>
          </true>
          <false>
            <value>0</value>
          </false>
        </boolean>
      </type>
      <value>0</value>
    </setting>
    <setting name='use_speed_number'>
      <type>
        <boolean>
          <true>
            <value>1</value>
          </true>
          <false>
            <value>0</value>
          </false>
        </boolean>
      </type>
      <value>1</value>
    </setting>
    <setting name='web_server'>
      <type>
        <boolean>
          <true>
            <value>1</value>
          </true>
          <false>
            <value>0</value>
          </false>
        </boolean>
      </type>
      <value>1</value>
    </setting>
    <setting name='telnet_server'>
      <type>
        <boolean>
          <true>
            <value>1</value>
          </true>
          <false>
            <value>0</value>
          </false>
        </boolean>
      </type>
      <value>1</value>
    </setting>
    <setting name='call_park'>
      <profileName>function call_park</profileName>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='call_pickup'>
      <profileName>function call_pickup</profileName>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='group_call_pickup'>
      <profileName>function group_call_pickup</profileName>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
    <setting name='direct_call_pickup'>
      <profileName>function direct_call_pickup</profileName>
      <type refid="ena-dis" />
      <value>disable</value>
    </setting>
  </group>
  <group name='PROVISION'>
    <setting name='type'>
      <type>
        <enum>
          <option>
            <label>tftp</label>
            <value>0</value>
          </option>
          <option>
            <label>https</label>
            <value>1</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>
    <setting name="cfg_download" hidden="yes">
      <type refid="ena-dis"></type>
      <value>enable</value>
    </setting>
    <setting name="sw_download" advanced="yes">
      <type refid="ena-dis"></type>
      <value>enable</value>
    </setting>
    <setting name='dhcp_get_tftp'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='https_url'>
      <value></value>
    </setting>
    <setting name='dhcp_get_http'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='https_port'>
      <type refid="port-number" />
      <value>443</value>
    </setting>
    <setting name='https_id'>
      <type>
        <string maxLen="50" />
      </type>
    </setting>
    <setting name='https_password'>
      <type>
        <string maxLen="50" />
      </type>
    </setting>
    <setting name='https_macform' hidden="yes">
      <type>
        <string maxLen="50" />
      </type>
      <value>$MAC</value>
    </setting>
    <setting name='verifying_cert'>
      <type>
        <integer min="0" max="7" />
      </type>
      <value>0</value>
    </setting>
    <setting name='smartup_use'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='smartup_type'>
      <type>
        <enum>
          <option>
            <value>interval</value>
          </option>
          <option>
            <value>daily</value>
          </option>
        </enum>
      </type>
      <value>daily</value>
    </setting>
    <setting name='smartup_interval'>
      <type>
        <integer min="0" max="86400" />
      </type>
      <value>3600</value>
    </setting>
    <setting name='smartup_time'>
      <type>
        <integer min="0" max="23" />
      </type>
      <value>0</value>
    </setting>
    <setting name='smartup_confirm'>
      <type refid="ena-dis" />
      <value>enable</value>
    </setting>
    <setting name='decrypt_key'>
      <type>
        <string maxLen="64"></string>
      </type>
      <value></value>
    </setting>
  </group>
  <group name='NETTIME'>
    <setting name='sntp_server_address'>
      <value />
    </setting>
    <setting name='interval'>
      <type>
        <integer min="0" max="120" />
      </type>
      <value>24</value>
    </setting>
    <setting name='timezone'>
      <type>
        <integer min="1" max="67" />
      </type>
      <value>3</value>
    </setting>
    <setting name='dst_auto_adjust'>
      <type refid="on-off" />
      <value>on</value>
    </setting>
    <setting name='dst_start_month'>
      <type refid="month-of-year" />
      <value>4</value>
    </setting>
    <setting name='dst_start_day'>
      <type>
        <integer min="0" max="31" />
      </type>
      <value>0</value>
    </setting>
    <setting name='dst_start_day_of_week'>
      <type refid="day-of-week" />
      <value>1</value>
    </setting>
    <setting name='dst_start_week_of_month'>
      <type>
        <integer min="1" max="7" />
      </type>
      <value>1</value>
    </setting>
    <setting name='dst_start_time'>
      <type>
        <integer min="0" max="23" />
      </type>
      <value>2</value>
    </setting>
    <setting name='dst_stop_month'>
      <type refid="month-of-year" />
      <value>10</value>
    </setting>
    <setting name='dst_stop_day'>
      <type>
        <integer min="0" max="31" />
      </type>
      <value>0</value>
    </setting>
    <setting name='dst_stop_day_of_week'>
      <type refid="day-of-week" />
      <value>1</value>
    </setting>
    <setting name='dst_stop_week_of_month'>
      <type>
        <integer min="1" max="7" />
      </type>
      <value>7</value>
    </setting>
    <setting name='dst_stop_time'>
      <type>
        <integer min="0" max="23" />
      </type>
      <value>2</value>
    </setting>
    <setting name='time_format'>
      <type>
        <enum>
          <option>
            <label>off</label>
            <value>0</value>
          </option>
          <option>
