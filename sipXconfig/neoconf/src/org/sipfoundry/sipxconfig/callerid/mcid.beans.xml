<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="maskCallerIdManager" class="org.springframework.aop.framework.ProxyFactoryBean" parent="abstractDao">
    <property name="target" ref="maskCallerIdManagerImpl"/>
    <property name="proxyInterfaces" value="org.sipfoundry.sipxconfig.callerid.MaskCallerIdManager"/>
  </bean>

  <bean id="maskCallerIdManagerImpl" class="org.sipfoundry.sipxconfig.callerid.MaskCallerIdManagerImpl">
    <property name="settingsDao">
      <bean parent="beanWithSettingsDao">
        <property name="target">
          <bean parent="beanWithSettingsDaoImpl">
            <constructor-arg value="org.sipfoundry.sipxconfig.callerid.MaskCallerIdSettings"/>
          </bean>
        </property>
      </bean>
    </property>
    <property name="sessionFactory">
      <ref bean="sessionFactory"/>
    </property>
    <property name="imdb" ref="imdb"/>
    <property name="daoEventPublisher" ref="daoEventPublisher"/>
  </bean>

  <bean id="mcidSettings" class="org.sipfoundry.sipxconfig.callerid.MaskCallerIdSettings" parent="settingsBean">
  </bean>
  
</beans>
