<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="natTraveral" class="org.sipfoundry.sipxconfig.nattraversal.NatTraversal">
    <property name="settingsDao">
      <bean parent="beanWithSettingsDao">
        <property name="target">
          <bean parent="beanWithSettingsDaoImpl">
            <constructor-arg value="org.sipfoundry.sipxconfig.nattraversal.NatSettings"/>
          </bean>
        </property>
      </bean>
    </property>
  </bean>

  <bean id="natSettings" class="org.sipfoundry.sipxconfig.nattraversal.NatSettings" scope="prototype">
    <property name="modelFilesContext" ref="modelFilesContext"/>
  </bean>

  <bean id="natConfig" class="org.sipfoundry.sipxconfig.nattraversal.NatConfiguration">
    <property name="velocityEngine" ref="velocityEngine" />
    <property name="nat" ref="natTraveral" />
  </bean>
</beans>
