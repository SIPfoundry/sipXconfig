<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

  <!-- Audiocodes TP260 -->
  <bean id="audiocodesTP" abstract="true"
    class="org.sipfoundry.sipxconfig.gateway.audiocodes.AudioCodesModel">
    <property name="beanId" value="gwAudiocodesTp260" />
    <property name="proxyNameSetting" value="SIP/ProxyName" />
    <property name="proxyIpSetting" value="SIP/ProxyIP" />
    <property name="profileTemplate" value="audiocodes/mp-gateway.ini.vm" />
  </bean>

  <bean id="audiocodesTP260" parent="audiocodesTP">
    <property name="label" value="AudioCodes TP260" />
    <property name="fxo" value="true" />
    <property name="fxs" value="false" />
    <property name="digital" value="true" />
  </bean>

  <bean id="gwAudiocodesTp260" class="org.sipfoundry.sipxconfig.gateway.audiocodes.Tp260Gateway"
    singleton="false" parent="gwGeneric">
  </bean>

  <!-- Mediant -->
  <bean id="audiocodesMP" abstract="true"
    class="org.sipfoundry.sipxconfig.gateway.audiocodes.AudioCodesModel">
    <property name="beanId" value="gwAudiocodesMediant" />
    <property name="proxyNameSetting" value="SIP/ProxyName" />
    <property name="proxyIpSetting" value="SIP/ProxyIP" />
    <property name="profileTemplate" value="audiocodes/mp-gateway.ini.vm" />
    <property name="digital" value="false" />
  </bean>

  <bean id="audiocodesMP1X4_4_FXO" parent="audiocodesMP">
    <property name="label" value="AudioCodes MP1X4 4 Port FXO" />
  </bean>

  <bean id="audiocodesMP1X8_8_FXO" parent="audiocodesMP">
    <property name="label" value="AudioCodes MP1X8 8 Port FXO" />
  </bean>
  
  <bean id="gwAudiocodesMediant" class="org.sipfoundry.sipxconfig.gateway.audiocodes.MediantGateway"
    singleton="false" parent="gwGeneric">
  </bean>

</beans>
