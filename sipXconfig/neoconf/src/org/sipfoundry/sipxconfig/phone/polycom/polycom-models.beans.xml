<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
  "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="polycom300" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 300/301"/>
    <property name="maxLineCount" value="2"/>
  </bean>
  <bean id="polycom430" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 430"/>
    <property name="maxLineCount" value="2"/>
  </bean>
  <bean id="polycom500" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 500/501"/>
    <property name="maxLineCount" value="3"/>
  </bean>
  <bean id="polycom550" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 550"/>
    <property name="maxLineCount" value="4"/>
  </bean>
  <bean id="polycom600" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 600/601"/>
    <property name="maxLineCount" value="12"/>
  </bean>
  <bean id="polycom650" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 650"/>
    <property name="maxLineCount" value="12"/>
  </bean>
  <bean id="polycom4000" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomModel">
    <property name="label" value="Polycom SoundPoint IP 4000"/>
    <property name="maxLineCount" value="1"/>
  </bean>
  <bean id="polycom" class="org.sipfoundry.sipxconfig.phone.polycom.PolycomPhone"
    singleton="false" parent="abstractPhone">
    <property name="tftpRoot" value="${sysdir.phone}/profile/tftproot"/>
    <property name="uploadManager" ref="uploadManager"/>
  </bean>
  <bean id="polycomFirmware" class="org.sipfoundry.sipxconfig.upload.UploadSpecification">
    <property name="beanId" value="zipUpload"/>
    <property name="modelFilePath" value="polycom/upload.xml"/>
    <property name="label" value="Polycom Soundpoint" />
  </bean>    
</beans>
