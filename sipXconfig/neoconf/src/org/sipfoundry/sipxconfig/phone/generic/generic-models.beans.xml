<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="genericPhoneModel" parent="abstractPhoneModel">
    <property name="beanId" value="genericPhone" />
    <property name="label" value="generic-phone (all models)" />
	<property name="maxLineCount" value="6"/>
    <property name="defaultProfileLocation" ref="httpProfileLocation" />
    <property name="profileTemplate" value="genericPhone/genericPhone.vm" />
    <property name="vendor" value="sipxecs" />
    <property name="serialNumberPattern" value="[a-zA-Z0-9]+"/>
    <property name="restartSupported" value="true" />
  </bean>

  <bean id="genericPhone" class="org.sipfoundry.sipxconfig.phone.generic.GenericPhone" parent="abstractPhone" scope="prototype">
  </bean>
</beans>
