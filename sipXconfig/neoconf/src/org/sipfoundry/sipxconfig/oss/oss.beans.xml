<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="osscoreSettings" class="org.sipfoundry.sipxconfig.oss.OSSCoreSettings" parent="settingsBean">
  </bean>

  <bean id="osscoreManager" class="org.springframework.aop.framework.ProxyFactoryBean" parent="abstractDao">
    <property name="target" ref="osscoreManagerImpl"/>
    <property name="proxyInterfaces" value="org.sipfoundry.sipxconfig.oss.OSSCoreManager"/>
  </bean>

  <bean id="osscoreManagerImpl" class="org.sipfoundry.sipxconfig.oss.OSSCoreManagerImpl">
    <property name="settingsDao">
      <bean parent="beanWithSettingsDao">
        <property name="target">
          <bean parent="beanWithSettingsDaoImpl">
            <constructor-arg value="org.sipfoundry.sipxconfig.oss.OSSCoreSettings"/>
          </bean>
        </property>
      </bean>
    </property>
    <property name="featureManager" ref="featureManager" />
  </bean>
    
  <bean id="osscoreConfiguration" class="org.sipfoundry.sipxconfig.oss.OSSCoreConfiguration">
    <property name="manager" ref="osscoreManager" />
  </bean>
  
</beans>
