<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="mwi" class="org.sipfoundry.sipxconfig.mwi.Mwi">
    <property name="settingsDao">
      <bean parent="beanWithSettingDao">
        <property name="target">
          <bean parent="beanWithSettingDaoImpl">
            <constructor-arg value="org.sipfoundry.sipxconfig.mwi.MwiSettings"/>
          </bean>
        </property>
      </bean>
    </property>
  </bean>
  
  <bean id="mwiConfig" class="org.sipfoundry.sipxconfig.mwi.MwiConfig">
    <property name="mwi" ref="mwi"/>
    <property name="velocityEngine" ref="velocityEngine"/>
  </bean>

  <bean id="mwiSettings" class="org.sipfoundry.sipxconfig.mwi.MwiSettings">
  </bean>
  
</beans>
