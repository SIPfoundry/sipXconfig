<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="acdStats" class="org.sipfoundry.sipxconfig.acd.stats.AcdStats">
    <property name="settingsDao" ref="beanWithSettingsDao" />
  </bean>
  
  <bean id="acdStatistics" class="org.sipfoundry.sipxconfig.acd.stats.AcdStatistics">
    <property name="acdContext" ref="acdContext" />
  </bean>
  
  <bean id="acdConfig" class="org.sipfoundry.sipxconfig.acd.stats.AcdConfig">
    <property name="acdStats" ref="acdStats" />
  </bean>

  <bean id="acdSettings" class="org.sipfoundry.sipxconfig.acd.stats.AcdSettings" scope="prototype">
    <property name="modelFilesContext" ref="modelFilesContext"/>
  </bean>

</beans>
