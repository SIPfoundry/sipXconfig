<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="freeswitchConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.FreeswitchConfiguration">
    <property name="featureFeature" ref="featureFeature" />
  </bean>
  <bean id="freeswitchConfigFile" abstract="true">
    <property name="velocityEngine" ref="velocityEngine" />
    <property name="freeswitchFeature" ref="freeswitchFeature" />
  </bean>
  <bean id="freeswitchDefaultConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.DefaultContextConfiguration" parent="">
    <property name="conferenceContext" ref="conferenceBridgeContext" />
    <property name="freeswitchExtensionCollector" ref="freeswitchExtensionCollector" />
    <property name="featureManager" ref="featureManager" />
  </bean>
  <bean id="freeswitchLocalStreamConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.LocalStreamConfiguration">
    <property name="musicOnHoldManager" ref="musicOnHoldManager" />
  </bean>
  <bean id="freeswitchLogfileConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.LogfileConfiguration">
  </bean>
  <bean id="freeswitchModulesConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.ModulesConfiguration">
  </bean>
  <bean id="freeswitchSofiaConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.SofiaConfiguration">
    <property name="domainManager" ref="domainManager" />
    <property name="coreContext" ref="coreContext" />
  </bean>
  <bean id="freeswitchSwitchConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.SwitchConfiguration">
  </bean>
  <bean id="freeswitchXmlRpcConfig" class="org.sipfoundry.sipxconfig.freeswitch.config.XmlRpcConfiguration">
  </bean>
</beans>
