<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="featureManagerImpl" class="org.sipfoundry.sipxconfig.feature.FeatureManagerImpl">
    <property name="jdbcTemplate" ref="configJdbcTemplate"/>
    <property name="sessionFactory">
      <ref bean="sessionFactory"/>
    </property>
  </bean>

  <bean id="featureManager" class="org.springframework.aop.framework.ProxyFactoryBean">
    <property name="proxyInterfaces" value="org.sipfoundry.sipxconfig.feature.FeatureManager"/>
    <property name="target" ref="featureManagerImpl"/>
  </bean>

</beans>
