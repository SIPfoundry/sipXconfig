            <stdout file="/dev/null" />
            <stderr file="/dev/null" />
            <stdin file="" />
            <start control="true">
                <execute command="/opt/sipx-3.0/bin/keepalive.sh" defaultdir="/opt/sipx-3.0/var/log/sipxpbx" />
            </start>
            <!-- If no execute, then kill -->
            <stop control="false" />
            <!-- If no execute, then stop-start -->
            <restart control="false" />
            <!-- <dependency>??</dependency> -->
