<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module>
	<source path="client"/>
    <source path="widget" />
    <source path="locale" />
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="com.google.gwt.i18n.I18N"/>
	<inherits name="com.google.gwt.http.HTTP"/>

    <!-- Inherit the debug GWT package SmartGwtDebug instead of     -->
    <!-- SmartGwt when running the code in a dev. environment.      -->
    <!-- This will output human readable JavaScript.                -->
    <!-- Inherit SmartGwtTools if you need access to the            -->
    <!-- SmartClient Developer Console                              -->
    <inherits name='com.smartgwt.SmartGwt'/>
    <!-- <inherits name="com.smartgwt.debug.SmartGwtDebug"/>        -->
    <!-- <inherits name="com.smartgwt.tools.SmartGwtTools"/>        -->

    <extend-property name="locale" values="it"/>
    <extend-property name="locale" values="en"/>
    <stylesheet src="gwtuserportal.css"/>
	<entry-point
		class="org.sipfoundry.sipxconfig.userportal.client.UserPhonebookSearch" />
</module>
